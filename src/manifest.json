{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
  "applications": {
    "gecko": {
      "id": "lookout@s3_fix_version",
      "strict_min_version": "68.0"
    }
  },
  "version": "2.0.4",
  "default_locale": "en",
  "author": "Dugite-Code",
  "homepage_url": "https://github.com/TB-throwback/LookOut-fix-version/",
  "icons": {
    "32": "chrome/skin/LOicon-32.png",
    "48": "chrome/skin/LOicon-48.png",
    "64": "chrome/skin/LOicon-64.png"
  },
  "experiment_apis": {
    "WindowListener": {
      "schema": "api/WindowListener/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["WindowListener"]],
        "script": "api/WindowListener/implementation.js"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "legacy": {
    "options": {
      "page": "chrome://lookout/content/options.xul",
      "open_in_tab": false
    }
  }
}
